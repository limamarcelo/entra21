package entities;

public class Paralelograma implements FiguraGeometrica{

	private double base;
	private double altura;
	
	public Paralelograma(double base, double altura) {
		setBase(base);
		setAltura(altura);
	}
	
	public void setBase(double base) {
		
	}
	
	public calcularArea() {
		return base * altura;
	}
	
	public calcularPerimetro() {
		return 2 * (base + altura);
	}
}