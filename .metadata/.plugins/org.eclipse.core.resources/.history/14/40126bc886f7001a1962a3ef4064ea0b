package entities;

public class Triangulo implements FiguraGeometrica{

	private double base;
	private double altura;
	
	public Triangulo(double base, double altura) {
		setBase(base);
		setAltura(altura);
	}
	
	public void setBase(double base) {
		this.base = base;
	}
	
	public double getBase() {
		return base;
	}
	
}
